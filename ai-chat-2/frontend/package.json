{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@capacitor/android": "^7.0.1",
    "@capacitor/core": "^7.0.1",
    "@capacitor/ios": "^7.0.1",
    "@capacitor/local-notifications": "^7.0.0",
    "@capacitor/push-notifications": "^7.0.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "dotenv": "^16.4.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "launch-ios-emulator": "source .env && npx cap run --live-reload --target $IOS_TARGET ios",
    "launch-android-emulator": "source .env && npx cap run --live-reload --target $ANDROID_TARGET ios",
    "dev:ios": "concurrently \"yarn dev:web\" \"yarn run launch-ios-emulator\"",
    "dev:web": "react-scripts start",
    "dev:android": "echo 'Coming soon'",
    "build:web": "react-scripts build",
    "build:ios": "yarn build:web && npx cap copy ios && npx cap open ios",
    "build:android": "echo 'Coming soon'",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@capacitor/cli": "^7.0.1",
    "concurrently": "^9.1.2"
  }
}
